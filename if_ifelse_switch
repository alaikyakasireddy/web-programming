package main

import (
"fmt")

type Contact struct {
	greeting string
	name     string
}

func Greeting(contact Contact, mygreetings func(string)) {
	myGreetingMas, myNameMas := CreateMessage(contact.name, contact.greeting, "howdy")

	mygreetings(myGreetingalaikya)
	mygreetings(myNamealaikya)
}
func CreateMessage(name string, greeting ...string) (myGreeting string, myName string) {
	myGreeting = greeting[1] + " " + name
	myName = "\nHey, " + name + "\n"
	return
}

func myPrint(s string) {
	fmt.Print(s)
}

func myPrintln(s string) {
	fmt.Println(s)
}

